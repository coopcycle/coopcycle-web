    "autoload": {
        "psr-4": { "": "src/" },
        "classmap": [ "app/AppKernel.php", "app/AppCache.php" ]
    },
    "autoload-dev": {
        "psr-4": { "Tests\\": "tests/" }
    },
    "require": {
        "php": "^7.1",
        "symfony/symfony": "3.4.*",
        "symfony/property-info": "3.3.*",
        "api-platform/core": "2.2.*",
        "doctrine/orm": "^2.5",
