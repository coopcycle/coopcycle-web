{% if form.pricingRuleSet is defined %}
  {% if form.pricingRuleSet.vars.choices|length == 0 %}
    <div
      class="alert alert-warning">{% trans with {'%editPricingRule%':admin_deliveries_pricing} %}restaurant.form.noPricingRule{% endtrans %}</div>
    <div class="hidden">
      {{ form_row(form.pricingRuleSet) }}
    </div>
  {% else %}
    {{ form_row(form.pricingRuleSet) }}
    {% if form.pricingRuleSet.vars.data is null %}
      <div
        class="alert alert-warning">{% trans %}form.store_type.pricing_rule_set.warning{% endtrans %}</div>
    {% endif %}
  {% endif %}
{% endif %}

